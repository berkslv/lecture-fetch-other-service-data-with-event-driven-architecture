{
  "ProjectName": "eventfy-products",
  "MessageBroker": {
    "UserName": "guest",
    "Password": "guest",
    "Host": "localhost",
    "VirtualHost": "/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=../../../products-db.sql"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "Enrich": [
      "WithMachineName",
      "WithEnvironmentName"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "ElasticSearch": {
    "Url": "http://localhost:9200"
  },
  "AllowedHosts": "*"
}
